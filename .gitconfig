[user]
	email = usagi@WonderRabbitProject.net
	name = Usagi Ito
[push]
	default = matching
[alias]
  push-current-branch = !git push origin `git rev-parse --abbrev-ref HEAD`
[color]
	ui = true
[merge]
	tool = meld
[core]
	editor = vim --noplugin -Z
	excludesfile = ~/.gitconfig.d/.gitignore
	quotepath = false
